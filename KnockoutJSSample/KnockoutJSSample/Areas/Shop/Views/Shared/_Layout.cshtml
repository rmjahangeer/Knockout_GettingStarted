<!DOCTYPE html>
<html>
<head>
    <title>@Page.Title</title>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <!-- Google font -->
    <link href="https://fonts.googleapis.com/css?family=Hind:400,700" rel="stylesheet">

    <!-- Bootstrap -->
    <link type="text/css" rel="stylesheet" href="~/Areas/Shop/Content/css/bootstrap.min.css" />

    <!-- Slick -->
    <link type="text/css" rel="stylesheet" href="~/Areas/Shop/Content/css/slick.css" />
    <link type="text/css" rel="stylesheet" href="~/Areas/Shop/Content/css/slick-theme.css" />

    <!-- nouislider -->
    <link type="text/css" rel="stylesheet" href="~/Areas/Shop/Content/css/nouislider.min.css" />

    <!-- Font Awesome Icon -->
    <link rel="stylesheet" href="~/Areas/Shop/Content/css/font-awesome.min.css">

    <!-- Custom stlylesheet -->
    <link type="text/css" rel="stylesheet" href="~/Areas/Shop/Content/css/style.css" />

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
        <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    @RenderSection("head", required: false)
</head>
<body>
    <!-- HEADER -->
    @Html.Partial("Partials/_Header")
    <!-- /HEADER -->

    @Html.Partial("Partials/_Categories")

    

    @RenderBody()
    <!-- FOOTER -->
    @Html.Partial("Partials/_Footer")
    <!-- /FOOTER -->
    <!-- jQuery Plugins -->
    <script src="~/Areas/Shop/Scripts/js/jquery.min.js"></script>
    <script src="~/Areas/Shop/Scripts/js/bootstrap.min.js"></script>
    <script src="~/Areas/Shop/Scripts/js/slick.min.js"></script>
    <script src="~/Areas/Shop/Scripts/js/nouislider.min.js"></script>
    <script src="~/Areas/Shop/Scripts/js/jquery.zoom.min.js"></script>
    <script src="~/Areas/Shop/Scripts/js/main.js"></script>

</body>
</html>
